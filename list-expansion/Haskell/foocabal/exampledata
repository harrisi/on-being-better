        <script type="text/javascript">
            function formatCurrency(n) {
                if (n !== null && n !== undefined) {
                    var myCurrency = n.toFixed(2).replace(/\d(?=(\d{3})+\.)/g, '$&,');
                    return "$" + myCurrency;
                } else {
                    return "$0.00";
                }
            }

            function addAnalyticsTrackingEvent(pageTitle) {
                ga('send', 'event', 'link', 'click', pageTitle);
            }

            // for a specific parameter, call getUrlVars()["myParam"]
            function getUrlVars() {
                var vars = [], hash;
                var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
                for (var i = 0; i < hashes.length; i++) {
                    hash = hashes[i].split('=');
                    vars.push(hash[0]);
                    vars[hash[0]] = hash[1];
                }
                return vars;
            }

            $(function () {
                // google analytics event tracking - ga object is defined in analytics.js included in _Layout
                $(".event-tracking").on('click change', function() {
                    // add specific info for Analytics reporting
                    addAnalyticsTrackingEvent($(this).data("page-title"));
                });

                // prevent clicking on an empty anchor
                $(document).on("click", ".disable-anchor", function(e) {
                    e.preventDefault();
                });

            });
        </script>

        



