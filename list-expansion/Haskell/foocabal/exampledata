<script type="text/javascript">
    var arrival = "A";
    var departure = "D";

    $(document).ready(function () {

        $("#flights-accordion").accordion({
            collapsible: true,
            heightStyle: "content",
            animate:100
        });

        $("#filter-arrivals").on("click", function () {
            filterDom(arrival);
        });

        $("#filter-departures").click(function () {
            filterDom(departure);
        });

        $("#filter-all").click(function () {
            showHidden();
        });
    });
    
    function showHidden() {
        $("#flight-table >tbody tr:hidden").show();
        $("#flights-accordion h3:hidden").show();
        $("#flights-accordion div:hidden").show();
        $("#flights-accordion").accordion({active: false});
        $("#flights-accordion").accordion('refresh');
    }

    function filterDom(type) {
        showHidden();
        $("#flight-table >tbody tr:not([class*='" + type + "'])").hide();
        $("#flights-accordion h3:not([class*='" + type + "'])").hide();
        $("#flights-accordion div:not([class*='" + type + "'])").hide();
    }
</script>





