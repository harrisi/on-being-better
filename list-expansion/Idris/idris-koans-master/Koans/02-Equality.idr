-- | Exercises on Equality.
module Koans.Equality

equalityEq : Bool
equalityEq = True == True

equalityNeq : Bool
equalityNeq = 5 /= 3

equalityGeq : Bool
equalityGeq = 5 >= 4

equalityGtr : Bool
equalityGtr = 5 > 4

equalityLeq : Bool
equalityLeq = 5 <= 5

equalityLess : Bool
equalityLess = 5 < 6

